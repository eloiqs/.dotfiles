// Place your settings in this file to overwrite the default settings
{
	"editor.fontSize": 19,
	"editor.tabSize": 2,
	"editor.fontFamily": "Operator Mono",
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"www/lib-bower/**/*": true,
		"www/lib": true,
		"www/pub/**": true,
		"dist/www/**": true,
		".vscode/**": true,
		"*/**.xml": true,
		".tscache/**/*": true
	},
	"workbench.activityBar.visible": true,
	"editor.insertSpaces": false,
	"workbench.colorTheme": "Oceanic Next",
	"workbench.iconTheme": "seti",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.minimap.renderCharacters": true,
	"editor.quickSuggestionsDelay": 0,
	"typescript.format.enable": false,
	"typescript.useCodeSnippetsOnMethodSuggest": true,
	"window.zoomLevel": 0,
	"search.useRipgrep": false,
	"editor.renderWhitespace": "none",
	"editor.detectIndentation": false,
	"typescript.implementationsCodeLens.enabled": true,
	"vim.handleKeys": {
		"<C-w>": false,
		"<C-\/>": false
	},
	"vim.autoindent": true,
	"vim.foldfix": true,
	"vim.surround": true,
	"editor.tabCompletion": true,
	"workbench.colorCustomizations": {
		"activityBar.background": "#1B2B34",
		"activityBar.foreground": "#D8DEE9",
		"activityBar.dropBackground": "#4F5B66",
		"activityBarBadge.background": "#99C794",
		"activityBarBadge.foreground": "#1B2B34",
		"sideBar.background": "#1B2B34",
		"sideBarSectionHeader.background": "#4F5B66",
		"sideBarTitle.foreground": "#D8DEE9",
		"tab.activeBackground": "#D8DEE9",
		"tab.activeForeground": "#1B2B34",
		"tab.border": "#1B2B34",
		"tab.inactiveBackground": "#1B2B34",
		"tab.inactiveForeground": "#D8DEE9",
		"editorGroupHeader.tabsBackground": "#1B2B34",
		"editorGroupHeader.noTabsBackground": "#1B2B34",
		"editorLineNumber.foreground": "#65737E",
		"editorSuggestWidget.background": "#1B2B34",
		"editorSuggestWidget.selectedBackground": "#343D46",
